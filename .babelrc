{
    "presets": [
        ["env", {
            "modules": false,
            "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            },
            "debug": false,
            "useBuiltIns": true
        }],
        "stage-2"
    ],
    "plugins": ["transform-runtime", ["component", {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
    }]]
}